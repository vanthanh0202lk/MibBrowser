--    mplsLdpSesPeerNextHopAddrType, mplsLdpSesPeerNextHopAddr, 
--    mplsFecIndexNext, mplsFecType, mplsFecAddrFamily, 
--    mplsFecAddrLength, mplsFecAddr, mplsFecStorType, 
--    mplsFecRowStatus

mplsLdpGenericGroup OBJECT IDENTIFIER ::= { mplsLdpGroups 2 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    Objects that apply to all MPLS LDP implementations
--    using Generic Lables.
--  objects
--    mplsLdpEntityConfGenIfIndxOrZero, 
--    mplsLdpEntityConfGenLRStorType, 
--    mplsLdpEntityConfGenLRRowStatus

mplsLdpAtmGroup OBJECT IDENTIFIER ::= { mplsLdpGroups 3 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    Objects that apply to all MPLS LDP implementations
--    over ATM.
--  objects
--    mplsLdpEntityAtmIfIndxOrZero, mplsLdpEntityAtmMergeCap, 
--    mplsLdpEntityAtmLRComponents, 
--    mplsLdpEntityAtmVcDirectionality, 
--    mplsLdpEntityAtmLsrConnectivity, 
--    mplsLdpEntityDefaultControlVpi, 
--    mplsLdpEntityDefaultControlVci, mplsLdpEntityUnlabTrafVpi, 
