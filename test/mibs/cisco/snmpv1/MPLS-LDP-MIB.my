mplsLdpMappingGroup OBJECT IDENTIFIER ::= { mplsLdpGroups 5 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    These objects are optional and only need to be supported
--    for LDP implementations which support the
--    tables, mplsInSegmentTable, mplsOutSegmentTable
--    and mplsXCTable, in the LSR MIB.
--  objects
--    mplsLdpSesInLabelType, mplsLdpSesInLabelConnType, 
--    mplsLdpSesOutLabelType, mplsLdpSesOutLabelConnType, 
--    mplsLdpSesOutSegmentIndex, mplsLdpSesXCIndex, 
--    mplsXCFecOperStatus, mplsXCFecOperStatusLastChange

mplsLdpNotificationsGroup OBJECT IDENTIFIER ::= { mplsLdpGroups 6 }
-- NOTIFICATION-GROUP
--  Status
--    mandatory
--  Descr
--    The notification(s) which an MPLS LDP implemention
--     is required to implement.
--  notifications
--    mplsLdpInitSesThresholdExceeded, mplsLdpPVLMismatch, 
--    mplsLdpSessionUp, mplsLdpSessionDown

mplsLdpModuleCompliance OBJECT IDENTIFIER ::= { mplsLdpCompliances 1 }
-- MODULE-COMPLIANCE
--  Status
--    mandatory
--  Descr
--    The basic implementation requirements for agents that
--    support the MPLS LDP MIB.
--  Module
--    >>current<<
--   MandGroup
--    mplsLdpGeneralGroup
--   MandGroup
--    mplsLdpNotificationsGroup
--   OptGroup
--    mplsLdpGenericGroup
--   OptGroup
--    mplsLdpAtmGroup
--   OptGroup
--    mplsLdpFrameRelayGroup
--   OptGroup
--    mplsLdpMappingGroup


